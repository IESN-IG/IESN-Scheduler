<% if (active === "section"){ %>
    <script>
        <% if(toCalendarURL.length > 0){ %>
        window.onload = function (event) {
            window.location.hash = "#calendarURL";
        };
        <% } %>

        $(function () {
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
            $('.selectpicker').selectpicker('mobile');
        }
    });
    $('.selectWithAllOption').on('changed.bs.select', function (e, clickedIndex, isSelected, previousValue) {
        if(clickedIndex === 0){
            for(let option of e.target.options){
                option.selected = isSelected;
            }
            $('.selectWithAllOption').selectpicker('refresh');
        }
    });
</script>
<%}%>
