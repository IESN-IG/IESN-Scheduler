{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": ["."],
        "exclude": ["node_modules", "docs"]
    },
    "opts": {
        "template": "node_modules/better-docs",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "readme": "./README.md"
    },
    "tags": {
        "allowUnknownTags": ["category"]
    },
    "plugins": [
        "node_modules/better-docs/category"
    ],
    "templates": {
        "search": true,
        "better-docs": {
            "name": "Documentation IESN Scheduler",
            "title": "Documentation IESN Scheduler",
            "navLinks": [
                {
                    "label": "Github",
                    "href": "https://github.com/IESN-IG/IESN-Scheduler"
                }
            ]
        }
    }
}